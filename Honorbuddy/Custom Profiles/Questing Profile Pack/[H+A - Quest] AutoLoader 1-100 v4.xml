				</If>
				<If Condition="Me.Race == WoWRace.Tauren" >
					<CustomBehavior File="LoadProfile" ProfileName="Classic\[H - Quest] 1-58 [Kick]\[H - Quest] 1-12 [Kick]\[H - Quest] Tauren 1-5 [Kick]" RememberProfile="true" />
